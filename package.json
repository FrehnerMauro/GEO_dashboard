{
  "name": "geo-platform",
  "version": "1.0.0",
  "description": "Production-ready GEO (Generative Engine Optimization) platform",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "db:migrate": "wrangler d1 migrations apply geo-db",
    "db:migrate:remote": "wrangler d1 migrations apply geo-db --remote",
    "db:create": "wrangler d1 create geo-db"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "cheerio": "^1.0.0",
    "openai": "^4.67.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.60.0"
  }
}
